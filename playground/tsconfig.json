{
  "extends": "../tsconfig-base.json",
  "include": ["src","src/**/*.d.ts","src/**/*.ts","src/**/*.js"],
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "resolveJsonModule": true,
    "sourceMap":true,
    "strict": false,
    "allowJs": true,
    "checkJs": true,
    "isolatedModules": true,
  }
}
